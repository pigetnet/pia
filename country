#!/bin/bash
piaCountry=$(cat /etc/openvpn/pia.conf  |grep "privateinternetaccess.com")
piaCountry=$(echo $piaCountry|sed s'/remote france.privateinternetaccess.com 1194/France/')
piaCountry=$(echo $piaCountry|sed s'/remote us-california.privateinternetaccess.com 1194/US California/')
piaCountry=$(echo $piaCountry|sed s'/remote us-east.privateinternetaccess.com 1194/US East/')
piaCountry=$(echo $piaCountry|sed s'/remote us-midwest.privateinternetaccess.com 1194/US Midwest/')
piaCountry=$(echo $piaCountry|sed s'/remote us-texas.privateinternetaccess.com 1194/US Texas/')
piaCountry=$(echo $piaCountry|sed s'/remote us-florida.privateinternetaccess.com 1194/US Florida/')
piaCountry=$(echo $piaCountry|sed s'/remote us-seattle.privateinternetaccess.com 1194/US Seattle/')
piaCountry=$(echo $piaCountry|sed s'/remote us-west.privateinternetaccess.com 1194/US West/')
piaCountry=$(echo $piaCountry|sed s'/remote us-siliconvalley.privateinternetaccess.com 1194/US Silicon Valley/')
piaCountry=$(echo $piaCountry|sed s'/remote us-newyorkcity.privateinternetaccess.com 1194/US New York City/')
piaCountry=$(echo $piaCountry|sed s'/remote uk-london.privateinternetaccess.com 1194/UK London/')
piaCountry=$(echo $piaCountry|sed s'/remote uk-southampton.privateinternetaccess.com 1194/UK Southampton/')
piaCountry=$(echo $piaCountry|sed s'/remote ca-toronto.privateinternetaccess.com 1194/CA Toronta/')
piaCountry=$(echo $piaCountry|sed s'/remote ca.privateinternetaccess.com 1194/CA North York/')
piaCountry=$(echo $piaCountry|sed s'/remote aus.privateinternetaccess.com 1194/AU Sydney/')
piaCountry=$(echo $piaCountry|sed s'/remote aus-melbourne.privateinternetaccess.com 1194/AU Melbourne/')
piaCountry=$(echo $piaCountry|sed s'/remote nl.privateinternetaccess.com 1194/Netherlands/')
piaCountry=$(echo $piaCountry|sed s'/remote swiss.privateinternetaccess.com 1194/Switzerland/')
piaCountry=$(echo $piaCountry|sed s'/remote sweden.privateinternetaccess.com 1194/Sweden/')
piaCountry=$(echo $piaCountry|sed s'/remote germany.privateinternetaccess.com 1194/Germany/')
piaCountry=$(echo $piaCountry|sed s'/remote ireland.privateinternetaccess.com 1194/Ireland/')
piaCountry=$(echo $piaCountry|sed s'/remote italy.privateinternetaccess.com 1194/Italy/')
piaCountry=$(echo $piaCountry|sed s'/remote russia.privateinternetaccess.com 1194/Russia/')
piaCountry=$(echo $piaCountry|sed s'/remote ro.privateinternetaccess.com 1194/Romania/')
piaCountry=$(echo $piaCountry|sed s'/remote turkey.privateinternetaccess.com 1194/Turkey/')
piaCountry=$(echo $piaCountry|sed s'/remote hk.privateinternetaccess.com 1194/Honk Kong/')
piaCountry=$(echo $piaCountry|sed s'/remote sg.privateinternetaccess.com 1194/Singapore/')
piaCountry=$(echo $piaCountry|sed s'/remote japan.privateinternetaccess.com 1194/Japan/')
piaCountry=$(echo $piaCountry|sed s'/remote israel.privateinternetaccess.com 1194/Isreal/')
piaCountry=$(echo $piaCountry|sed s'/remote mexico.privateinternetaccess.com 1194/Mexico/')
piaCountry=$(echo $piaCountry|sed s'/remote brazil.privateinternetaccess.com 1194/Brazil/')
echo $piaCountry
